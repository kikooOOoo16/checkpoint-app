{
  "name": "checkpoint-api",
  "version": "1.0.0",
  "description": "A simple checkpoint API with an EJS template UI",
  "engines": {
    "node": ">=20"
  },
  "main": "./src/app.js",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx . --fix",
    "start": "node .",
    "start:local": "set NODE_ENV=LOCAL && node -r dotenv/config .",
    "build": "tsc",
    "migrate:v0.1": "ts-node -r dotenv/config migration/v0.1/create_collections.ts",
    "migrate": "npm run migrate:v0.1",
    "server:watch": "ts-node-dev --respawn --transpile-only -r dotenv/config src/app.ts",
    "prestart": "yarn build",
    "test:unit": "jest --config=jest.unit.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jestSonar": {
    "reportPath": "test/coverage",
    "reportFile": "sonar-test-report.xml"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "ajv": "8.16.0",
    "ajv-errors": "3.0.0",
    "applicationinsights": "2.9.0",
    "axios": "1.13.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "config": "3.3.9",
    "cors": "2.8.5",
    "express": "4.21.2",
    "express-http-context": "1.2.4",
    "http-status-codes": "2.3.0",
    "log4js": "6.9.1",
    "luxon": "3.4.3",
    "jsonpath": "1.1.1",
    "json-bigint": "1.0.0",
    "multer": "2.0.2",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.11.3",
    "swagger-ui-express": "5.0.0",
    "typescript-string-operations": "1.5.0",
    "js-yaml": "4.1.1",
    "ejs": "3.1.10",
    "mongodb": "6.5.0",
    "express-session": "1.18.0"
  },
  "devDependencies": {
    "dotenv": "16.4.5",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/config": "3.3.2",
    "@types/cors": "2.8.15",
    "@types/express": "4.17.25",
    "@types/express-session": "1.18.2",
    "@types/jest": "30.0.0",
    "@types/jsonpath": "0.2.2",
    "@types/js-yaml": "4.0.8",
    "@types/luxon": "3.3.3",
    "@types/multer": "1.4.9",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-extended": "4.0.2",
    "jest-mock-extended": "3.0.5",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "3.0.3",
    "prettier-eslint": "16.1.2",
    "pretty-quick": "4.1.1",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.2"
  }
}
